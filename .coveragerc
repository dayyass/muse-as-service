[run]
branch = True
source = src

[report]
exclude_lines =
    pragma: no cover
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

omit =
    src/muse_as_service/utils.py
    src/muse_as_service/database/add_user.py
    src/muse_as_service/database/remove_user.py

show_missing = True
ignore_errors = False
